{
  "name": "swappi",
  "version": "0.1.0",
  "author": "soofka",
  "description": "swappi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/soofka/swappi"
  },
  "bugs": {
    "url": "https://github.com/soofka/swappi/issues"
  },
  "homepage": "https://github.com/soofka/swappi#readme",
  "main": "dist/index.js",
  "files": [
    "dist",
    "examples"
  ],
  "scripts": {
    "test": "node test.js",
    "build": "npm run test && node build.js",
    "start": "npm run build && node dist/index.js",
    "generate-basic": "npm run start -- -g examples/basic/templates",
    "build-basic": "npm run start -- -c examples/basic/config.js -b",
    "run-basic": "npm run start -- -r examples/basic/dist",
    "watch-basic": "npm run start -- -c examples/basic/config.js -w",
    "watch-run-basic": "npm run start -- -c examples/basic/config.js -w -r",
    "generate-full": "npm run start -- -g examples/full/templates",
    "build-full": "npm run start -- -c examples/full/config.js -b",
    "run-full": "npm run start -- -r examples/full/dist",
    "watch-full": "npm run start -- -c examples/full/config.js -w",
    "watch-run-full": "npm run start -- -c examples/full/config.js -w -r"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "clean-css": "^5.3.3",
    "css": "^3.0.0",
    "es-main": "^1.3.0",
    "html-minifier": "^4.0.0",
    "mime-db": "^1.52.0",
    "open": "^10.0.3",
    "sharp": "^0.33.2",
    "terser": "^5.27.0"
  },
  "devDependencies": {
    "prettier": "3.2.4"
  },
  "type": "module"
}
